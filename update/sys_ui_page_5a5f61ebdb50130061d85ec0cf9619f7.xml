        <direct>false</direct>
        <endpoint>x_58872_needit_Angular Test Page 02.do</endpoint>
        <html><![CDATA[<?xml version="1.0" encoding="utf-8" ?>
<j:jelly trim="false" xmlns:j="jelly:core" xmlns:g="glide" xmlns:j2="null" xmlns:g2="null">
   <head>
      <title>Angular JS Expressions</title>
      <script src = "https://ajax.googleapis.com/ajax/libs/angularjs/1.3.14/angular.min.js"></script>
   </head>
	<body>
		<form id="form1">
			<div ng-app="">
				<table class="auto-style">
				   <tr>
					   <td class="auto-style2">First Name</td>
					   <td>
						   <input type="text" ng-model="FirstName"/>
					   </td>
				   </tr>
				   <tr>
					   <td class="auto-style2">Last Name</td>
					   <td>
						   <input id="text2" type="text" ng-model="LastName"/>
					   </td>
				   </tr>
				   <tr>
					   <td class="auto-style2">Quantity</td>
					   <td>
						   <input id="text3" type="text" ng-model="Quantity"/>
					   </td>
				   </tr>
				   <tr>
					   <td class="auto-style2">Cost</td>
					   <td>
						   <input id="text4" type="text" ng-model="Cost" value="50"/>
					   </td>
				   </tr>
				   <tr>
					   <td class="auto-style2">
						   <input type="button" value="Click Here" ng-click="message = FirstName + ' ' + LastName + ' has to pay ' + Cost * Quantity"/>
					   </td>
					   <td>
						   {{ message }}
					   </td>
				   </tr>
			   </table>
			</div>
		</form>
	</body>
	
</j:jelly>]]></html>
